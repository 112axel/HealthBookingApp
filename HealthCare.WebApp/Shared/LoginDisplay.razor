@inject AccountService AccountService
<AuthorizeView>
    <Authorized>
    </Authorized>
    <NotAuthorized>
    </NotAuthorized>
</AuthorizeView>


@code{

    private string GetName(string? userName)
    {
        if(userName == null)
        {
            return "null";
        }
        var account = AccountService.GetAccountByName(userName);
        return $"{account.FirstName} {account.LastName}";
    }


}
